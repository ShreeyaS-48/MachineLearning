    "dt = DecisionTreeClassifier(random_state=42,class_weight='balanced')\n",
    "rf = RandomForestClassifier(random_state=42,n_jobs=-1,class_weight='balanced')\n",
    "cv = StratifiedKFold(n_splits=5,shuffle=True,random_state=42)"
